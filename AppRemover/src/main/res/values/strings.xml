	<!-- My application is just installed. And haven't notices anther application running. -->
	<string name="havent_seen_app_running">Haven\'t seen it running</string>
	<!-- Havent noticed application is running, during observation period -->
	<string name="unused_at_least_X">Unused at least\n%1$s</string>
	
	<!-- Notification header. Unused apps found. Where apps substituted from plural applications form. -->
	<string name="notification_unused_header">Unused %1$s found</string>
	<!-- Notification body id consumed size known. 10 apps consuming 163Mb. Where %1$d and %3$s is quantity and size. And apps is substituted from app plurals -->
	<string name="notification_unused_apps_consuming">%1$d %2$s consuming %3$s</string>
	<!-- Notification body id consumed size unknown. Example: 10 apps. (see apps plural) -->
	<string name="notification_unused_apps">%1$d %2$s</string>
	
	<!-- Unknown size of application. Used instead 11Mb, for example, when size is not determined-->
	<string name="app_size_unknown">Unknown</string>
	
	<!-- On the unused apps screen. Tells to user, that my application is looking for unused applications now -->
	<string name="unused_empty_monitoring_apps">Monitoring applications activity to learn which are unused</string>
	<!-- This program will notify, when unused applications will be found -->
	<string name="unused_empty_will_notify">I will notify you when unused applications will be found</string>
	
	<!-- Android toast message. Shown when application can't be launched directly. -->
	<string name="toast_cant_launch_app">Sorry, can\'t launch this application</string>
	<!-- Android toast message. Shown, when user sets "don't notify" mark". Will not prompt 
	user to remove application if it appears to be unused-->
	<string name="toast_dont_notify">Will not notify you about this application</string>
	
	<!-- plural - work forms for different quantities.
	For example: one minute, few - minutes, many - minutes, other - 3.5 minutes -->
	
	<!-- Plural forms of abbreviated word "application" -->
	<plurals name="application_count">
		<item quantity="one">app</item>
		<item quantity="other">apps</item>
	</plurals>
	
	<!-- Plural forms of abbreviated word "minute". Example: 1min, 5mins -->
	<plurals name="minute_count">
		<item quantity="one">%1$dmin</item>
		<item quantity="other">%1$dmins</item>
	</plurals>
	
	<!-- Plural forms of abbreviated word "hour". Example: 1hr, 5hrs -->
	<plurals name="hour_count">
		<item quantity="one">%1$dhr</item>
		<item quantity="other">%1$dhrs</item>
	</plurals>
	
	<!-- Plural forms of word "day". Example: 1 day, 5 days -->
	<plurals name="day_count">
		<item quantity="one">%1$d day</item>
		<item quantity="other">%1$d days</item>
	</plurals>
	
